                                     BTP(CakePos)
                                     elseif (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - CakePos.Position).Magnitude < 1500 then
                                     toTarget(CakePos)
                                     end
                                 else
                                     toTarget(CakePos)
                                 end
